<!DOCTYPE html>
<html>
  <head>
    <title>Buddy Search Results</title>
    
    <link rel='stylesheet' href="/stylesheets/buddy_search_style.css" />

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
<!--     <script src="https://cdnjs.cloudflare.com/ajax/libs/handlebars.js/2.0.0/handlebars.js"></script> -->

    <script src="/js/buddy_search.js"></script>

  </head>
  <body>
    <div id="top-bar">Placeholder for top bar</div>
    <div id="filter-by-container">
      <form>
        <h1>Filter By</h1>
      </form>
    
    </div><div id="search-results-container">
<!--       <script id="hbtemplate" type="text/x-handlebars-template"> -->
      {{#each users}}
        <div class="search-result">
          <div class="user" rel="{{userID}}">
            <div class="user-image-container">
              <img class="user-image" src="{{userImageURL}}">
            </div><div class="username-container">
              <h1 class="username">{{username}}</h1>
            </div>
            {{#each tripImages}}
              <img class="trip-image" src="{{tripImageURL}}">
            {{/each}}
          </div>
        </div>
      {{/each}}
<!--       </script> -->
    </div>

  </body>
</html>

